{
  "name": "marknote",
  "version": "0.0.1",
  "description": "Markdown editor app for evernote",
  "main": "dist/main/index.js",
  "scripts": {
    "start": "NODE_ENV=development concurrently -k -p \"[{name}]\" -n \"main,renderer,electron\" -c \"bgBlue.white.bold,bgGreen.white.bold,yellow.bold\" \"npm run watch:main\" \"npm run build:renderer\" \"npm run start:main\" ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:main": "electron .",
    "watch:main": "tsc -w --skipLibCheck -p ./src/main",
    "build:renderer": "webpack --config webpack.config.js",
    "build:main": "tsc --locale zh-CN -p ./src/main --skipLibCheck --sourceMap false",
    "clean": "rm -rf ./dist/main",
    "main": "electron .",
    "build": "npm run clean & npm run build:main & npm run build:renderer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moyus/marknote.git"
  },
  "keywords": [
    "markdown",
    "evernote"
  ],
  "author": "linkFly6@live.com,moyuboy@gmail.com",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/moyus/marknote/issues"
  },
  "homepage": "https://github.com/moyus/marknote#readme",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "concurrently": "^3.6.1",
    "css-loader": "^1.0.0",
    "electron": "^2.0.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "^3.9.1",
    "gulp-watch": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "history": "^4.7.2",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  }
}
