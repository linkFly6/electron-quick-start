  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/moyus/marknote/issues"
  },
  "homepage": "https://github.com/moyus/marknote#readme",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "electron": "^2.0.6",
    "gulp": "^3.9.1",
    "gulp-watch": "^5.0.1",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  }
}
